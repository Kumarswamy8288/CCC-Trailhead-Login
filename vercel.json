{
  "version": 2,
  "builds": [
    {
      "src": "CCC PORTAL/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "CCC PORTAL/dist"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": "CCC PORTAL/dist"
}
